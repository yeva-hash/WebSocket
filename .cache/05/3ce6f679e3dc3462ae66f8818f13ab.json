nect\", ()=> {\r\n                    socket.on(\"registration_completed\", () => {\r\n                    console.log(`I'm registered`)\r\n                })\r\n    \r\n            \r\n                // this.model.setActiveUsers()\r\n            socket.on(\"new_user_connected\", (socketId) => {\r\n                console.log(`new user connected --> ${socketId}`)\r\n            })\r\n            socket.on(\"user_registered\", (username, socketId) => {\r\n                // this.model.setActiveUsers()\r\n                // this.view.renderUser(username)\r\n                console.log(`${socketId} registered as ${username}`)\r\n            })\r\n            socket.on('user_joined_room', (userId,roomId) => {console.log(`user${userId} joined to room with id --> ${roomId}`)})\r\n            // socket.on(\"user_disconnected\", (socketId) => {\r\n            //     this.view.deleteDisconnectedUser(this.model.activeUsers, socketId)\r\n            // })\r\n\r\n            })\r\n}\r\n    handlRegis = (username:string|undefined) => {\r\n        this.view.renderUser(username)\r\n        this.model.getChannels(this.view.renderChannels)\r\n        socket.emit(\"register\", username)\r\n    }   \r\n    handlJoinedToRoom = (ROOM_ID:string|undefined) => {\r\n        this.model.setActiveUsers()\r\n        this.model.setActiveUsersForRoom(ROOM_ID)\r\n        socket.emit('join_room', (ROOM_ID))\r\n        console.log(this.model.activeUsers, this.model.activeUsersForRoom)\r\n        // this.view.renderUsersList(this.model.activeUsers, this.model.activeUsersForRoom)        \r\n    }\r\n}\r\n// Controller.getInstance()"},"lineCount":null}},"error":null,"hash":"7f344f25e35abde8da6437d9d337db61","cacheData":{"env":{}}}